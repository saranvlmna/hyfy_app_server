{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}